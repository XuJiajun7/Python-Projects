import os
import random
#The client uses events to make it work
import discord 

class HashTable:
  def __init__():
    

client = discord.Client() #connection to discord ?
@client.event #this registers an event
async def on_ready():
  print('Jia bot is logged in {0.user}'.format(client))
  #this is called when the bot is ready to start being used

@client.event
async def on_message(message):
  if message.author == client.user:
    return
  if message.content.startswith("$hello"):
    if message.author.id == 6335:
      await message.channel.send("hi jun!")
    else:
      await message.channel.send("hello")
  if message.content.startswith("$play"):
    rand = random.randint(0, 20)
    await message.channel.send(rand)
    #if the message is 'play' then print a random number
    
client.run(os.getenv("jiaTOKEN"))
